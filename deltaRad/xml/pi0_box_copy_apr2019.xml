    <var def="sqrt(2.0*reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]*reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]*(1.0-cos((acos(reco_shower_dirx[0]*reco_shower_dirx[1] + reco_shower_diry[0]*reco_shower_diry[1] + reco_shower_dirz[0]*reco_shower_dirz[1])))))/1000" binning = "(16, 0, 0.25)" unit = "Reconstructed Diphoton Mass [GeV]" type = "d" spectator = "false" />

