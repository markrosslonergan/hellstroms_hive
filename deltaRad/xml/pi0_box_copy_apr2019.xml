    <var def = "(reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]*reco_shower_dirx[(reco_shower_ordered_energy_index[0])]+reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]*reco_shower_dirx[(reco_shower_ordered_energy_index[1])])/1000" binning = "(16, -0.5, 0.5)" unit = "Reco. #pi^{0} x-Momentum [GeV]" type = "d" spectator = "true" />
