<?xml version="1.0" ?>

<!--General Precuts -->
<precut def ="Sum$(reco_flash_total_pe_in_beamgate) > 20" name ="Optical Filter 20 PE"/> 
<precut def = "(reco_vertex_x > 10 &amp;&amp; reco_vertex_x < 246 &amp;&amp; reco_vertex_y > -107 &amp;&amp; reco_vertex_y < 107 &amp;&amp; reco_vertex_z > 10 &amp;&amp; reco_vertex_z < 1026) " name = "Fiducial Vertex Cut" />
<!-- Track Precuts -->
<precut def = "reco_track_good_calo_best_plane[0]!=0 " name = "Track Good Calo Cut" /> 
<precut def = "(reco_track_startx[0] > 10 &amp;&amp; reco_track_startx[0] < 256-10 &amp;&amp; reco_track_starty[0] > -117+10 &amp;&amp; reco_track_starty[0] < 117-10 &amp;&amp; reco_track_endz[0] > 10 &amp;&amp; reco_track_endz[0] < 1036-10) &amp;&amp; (reco_track_endx[0] > 10 &amp;&amp; reco_track_endx[0] < 256-10 &amp;&amp; reco_track_endy[0] > -117+10 &amp;&amp; reco_track_endy[0] < 117-10 &amp;&amp; reco_track_endz[0] > 10 &amp;&amp; reco_track_endz[0] < 1036-10)" name = "Contained Track Cut" />
<!-- <precut def = "reco_track_displacement[0]<500" name = "Track Length Cut" /> -->

<!--Shower Precuts -->
<precut def = "(reco_shower_dEdx_amalgamated[0]==reco_shower_dEdx_amalgamated[0] &amp;&amp; reco_shower_dEdx_amalgamated[1]==reco_shower_dEdx_amalgamated[1])" name = "Shower dE/dx Good Calo"/> 
<precut def = "acos(reco_shower_dirx[0]*reco_shower_dirx[1]+reco_shower_diry[0]*reco_shower_diry[1]+reco_shower_dirz[0]*reco_shower_dirz[1])>25*3.14159/180.0" name = "Showers Not Overlapping" />
<!-- <precut def = "(reco_shower_dEdx_plane2_nhits[0]>1 &amp;&amp; reco_shower_dEdx_plane2_nhits[1]>1)" name = "Shower dE/dx Plane Min Nhits"/> -->
<!-- <precut def = "(reco_shower_dEdx_plane2_nhits[0]>1 || reco_shower_dEdx_plane0_nhits[0]>1 || reco_shower_dEdx_plane1_nhits[0]>1)" name = "Shower dE/dx Plane Min Nhits"/> -->

<!-- Stuff I added or edited -->
<precut def = "reco_shower_energy_plane2[0]>50. &amp;&amp; reco_shower_energy_plane2[1]>20." name = "Shower Energy Cut" /> 
<!-- <precut def = "reco_shower_energy_plane2[reco_shower_ordered_energy_index[0]]>50. &amp;&amp; reco_shower_energy_plane2[reco_shower_ordered_energy_index[1]]>20." name = "Shower Energy Cut" /> -->
<!-- <precut def="reco_shower_conversion_distance[0]<200&amp;&amp;reco_shower_conversion_distance[1]<200" name="Shower Conversion Distance Cut"/> -->

<!--
<precut def = "reco_shower_energy_max[0]>0.125" name = "Shower Energy Cut 100 mev" />
<precut def = " (reco_track_dirx[0]*reco_shower_dirx[0]+reco_track_diry[0]*reco_shower_diry[0]+reco_track_dirz[0]*reco_shower_dirz[0]) >-0.99  &amp;&amp; (reco_track_dirx[0]*reco_shower_dirx[0]+reco_track_diry[0]*reco_shower_diry[0]+reco_track_dirz[0]*reco_shower_dirz[0]) < 0.99" name = "Back to Back track shower cut"/>
<precut def = "reco_track_good_calo_best_plane[0]!=-1 " name = "Track Good Calo Cut" />
<precut def = "reco_shower_conversion_distance[0] < 100" name = "Shower Conversion Dist Cut" />
<precut def = "(0.938272*0.938272+2.0*((reco_track_proton_kinetic_energy[0]+0.938272)*(reco_shower_energy_max[0]*0.001)-sqrt((reco_track_proton_kinetic_energy[0]+0.938272)*(reco_track_proton_kinetic_energy[0]+0.938272)-0.938272*0.938272)*(reco_shower_energy_max[0]*0.001)*(reco_track_dirx[0]*reco_shower_dirx[0]+reco_track_diry[0]*reco_shower_diry[0]+reco_track_dirz[0]*reco_shower_dirz[0]))) >1.0" name = "Invariant Mass"/>
<precut def = "reco_track_mean_trunc_dEdx_start_half_plane0[0]/reco_track_mean_trunc_dEdx_end_half_plane0[0] <1.1" name = "Ratio plane 0"/>
<precut def = "(reco_track_proton_kinetic_energy[0]+0.938272)*(reco_track_diry[0])+(reco_shower_energy_max[0]*0.001)*reco_shower_diry[0] < 0.9" name = "Reconstructed Delta Momentum Y [GeV]" />
<precut def = "(cos(reco_track_theta_yz[0]) > 0.5)" name = "cos track"/> 
<precut def = "(reco_track_dirx[0]*reco_shower_dirx[0]+reco_track_diry[0]*reco_shower_diry[0]+reco_track_dirz[0]*reco_shower_dirz[0]) > -0.8" name="Cosine Angle between photon and proton [rad]" />
<precut def = "reco_track_endy[0] < 90" name = "track end"/>
<precut def = "reco_track_proton_kinetic_energy[0] < 0.6" name = "proton KE"/>
-->

<!--Comsic BDT Training + Spectator Variables-->
<mva type="BDT" use="yes" tag = "COSMIC" name ="Cosmic focused BDT" binning = "(18,0.12,0.88)">
    <method type = "COSMIC" use = "yes">
        <param>  NTrees=50 	</param>    <!-- Was 100 -->
        <param>  MinNodeSize=2.5% </param>				
        <param>  BoostType=RealAdaBoost</param>
        <param>  AdaBoostBeta=0.1 	</param>  <!-- Was 0.75 -->
        <param>  SeparationType=GiniIndex</param>				
        <param>  UseBaggedBoost</param> 
        <param>  BaggedSampleFraction=0.5	</param>				
        <param>  nCuts=-1	</param>				
        <param>  MaxDepth=2 	</param>   <!-- Was 4 -->	
    </method>
    <!--Vertex Variables-->
    <var def = "sqrt( pow(mctruth_nu_vertex_x-reco_vertex_x,2)+ pow(mctruth_nu_vertex_y-reco_vertex_y,2)+ pow(mctruth_nu_vertex_z-reco_vertex_z,2))" binning = "(16,0,100)" unit = "Reco-True Vertex Dist [cm]" type = "d" spectator = "true" />
    <var def = "reco_shower_flash_shortest_distz[(reco_shower_ordered_energy_index[0])]" binning = "(16,0,520)" unit = "Dist. from Leading Shower to Flashcenter Z [cm]" type = "d" spectator = "true" />
    <var def = "reco_shower_flash_shortest_distz[(reco_shower_ordered_energy_index[1])]" binning = "(16,0,520)" unit = "Dist. from Subeadling Shower to Flashcenter Z [cm]" type = "d" spectator = "true" />
    <var def = "reco_num_flashes[0]" binning = "(16,0,4)" unit = "Reconstructed Number of Flashes" type = "i" spectator = "true" /> 
    <var def = "reco_flash_total_pe[0]" binning = "(16,0,10000)" unit = "Reconstructed Flash Total PE" type = "d" spectator = "true" />
    <var def = "reco_flash_time[0]" binning = "(16,-30,30)" unit = "Reconstructed Flash Time" type = "d" spectator = "true" />
    <var def = "reco_flash_zcenter[0]" binning = "(16,-10,+1060)" unit = "Reconstructed Flash Z Center" type = "d" spectator = "true" />
    <var def = "reco_vertex_x[0]" binning = "(16,-10,260)" unit = " Reconstructed vertex X pos [cm]" type = "d" spectator = "true"/>
    <var def = "reco_vertex_y[0]" binning = "(16,-120,+120)" unit = " Reconstructed vertex Y pos [cm]" type = "d" spectator = "true"/>
    <var def = "reco_vertex_z[0]" binning = "(16,-10,1060)" unit = " Reconstructed vertex Z pos [cm]" type = "d" spectator = "true"/>
    <var def = "reco_shower_conversion_distance[(reco_shower_ordered_energy_index[0])]" binning = "(16,0,500)" unit = " Reco. Leading Shower conversion distance [cm]" type = "d" spectator = "false"/>
    <var def = "reco_shower_conversion_distance[(reco_shower_ordered_energy_index[1])]" binning = "(16,0,500)" unit = " Reco. Subleading Shower conversion distance [cm]" type = "d" spectator = "false"/>
    <var def = "reco_vertex_to_nearest_dead_wire_plane0[0]" binning = "(16,0,125)" unit = " Reconstructed Vertex dead wire dist plane 0 pos [cm]" type = "d" spectator = "true" />
    <var def = "reco_vertex_to_nearest_dead_wire_plane1[0]" binning = "(16,0,110)" unit = " Reconstructed Vertex dead wire dist plane 1 pos [cm]" type = "d" spectator = "true" />
    <var def = "reco_vertex_to_nearest_dead_wire_plane2[0]" binning = "(16,0,200)" unit = " Reconstructed Vertex dead wire dist plane 2 pos [cm]" type = "d" spectator = "true" />

    <!-- Shower Calo Variables-->
    <var def = "reco_shower_dEdx_plane0_median[(reco_shower_ordered_energy_index[0])]" binning = "(16,0,6.5)" unit = "Median Leading Shower dE/dx plane 0 [MeV/cm]" type="d" spectator="true"/>
    <var def = "reco_shower_dEdx_plane1_median[(reco_shower_ordered_energy_index[0])]" binning = "(16,0,6.5)" unit = "Median Leading Shower dE/dx plane 1 [MeV/cm]" type="d" spectator="true"/>
    <var def = "reco_shower_dEdx_plane2_median[(reco_shower_ordered_energy_index[0])]" binning = "(16,0,6.5)" unit = "Median Leading Shower dE/dx plane 2 [MeV/cm]" type="d" spectator="true"/>

    <var def = "reco_shower_dEdx_plane0_median[(reco_shower_ordered_energy_index[1])]" binning = "(16,0,6.5)" unit = "Median Subleading Shower dE/dx plane 0 [MeV/cm]" type="d" spectator="true"/>
    <var def = "reco_shower_dEdx_plane1_median[(reco_shower_ordered_energy_index[1])]" binning = "(16,0,6.5)" unit = "Median Subleading Shower dE/dx plane 1 [MeV/cm]" type="d" spectator="true"/>
    <var def = "reco_shower_dEdx_plane2_median[(reco_shower_ordered_energy_index[1])]" binning = "(16,0,6.5)" unit = "Median Subleading Shower dE/dx plane 2 [MeV/cm]" type="d" spectator="true"/>

    <var def = "reco_shower_dEdx_amalgamated[(reco_shower_ordered_energy_index[0])]" binning = "(16,0,6.5)" unit = "Median Leading Shower dE/dx Amalgamated [MeV/cm]" type="d" spectator="true"/>
    <var def = "reco_shower_dEdx_amalgamated[(reco_shower_ordered_energy_index[1])]" binning = "(16,0,6.5)" unit = "Median Subleading Shower dE/dx Amalgamated [MeV/cm]" type="d" spectator="true"/>

    <var def = "reco_shower_energy_max[(reco_shower_ordered_energy_index[0])]/1000.0" binning = "(16,0,0.6)" unit = "Reconstructed Leading Photon Energy [GeV]" type="d" spectator="true"/> 
    <var def = "reco_shower_energy_max[(reco_shower_ordered_energy_index[1])]/1000.0" binning = "(16,0,1.0)" unit = "Reco. Subleading Shower Energy [GeV]" type="d" spectator="true"/>

    <var def = "reco_shower_energy_plane0[(reco_shower_ordered_energy_index[0])]/1000.0" binning = "(16,0,1.0)" unit = "Reco. Leading Shower Energy Plane 0 [GeV]" type="d" spectator="true"/>
    <var def = "reco_shower_energy_plane1[(reco_shower_ordered_energy_index[0])]/1000.0" binning = "(16,0,1.0)" unit = "Reco. Leading Shower Energy Plane 1 [GeV]" type="d" spectator="true"/>
    <var def = "reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]/1000.0" binning = "(16,0,1.0)" unit = "Reco. Leading Shower Energy Plane 2 [GeV]" type="d" spectator="false"/>

    <var def = "reco_shower_energy_plane0[(reco_shower_ordered_energy_index[1])]/1000.0" binning = "(16,0,1.0)" unit = "Reco. Subleading Shower Energy Plane 0 [GeV]" type="d" spectator="true"/>
    <var def = "reco_shower_energy_plane1[(reco_shower_ordered_energy_index[1])]/1000.0" binning = "(16,0,1.0)" unit = "Reco. Subleading Shower Energy Plane 1 [GeV]" type="d" spectator="true"/>
    <var def = "reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]/1000.0" binning = "(16,0,1.0)" unit = "Reco. Subleading Shower Energy Plane 2 [GeV]" type="d" spectator="false"/>

    <var def = "reco_shower_dQdx_plane0_median[(reco_shower_ordered_energy_index[(reco_shower_ordered_energy_index[0])])]" binning = "(16,0,400000)" unit = "Median Shower dQ/dx plane 0" type="d" spectator="true"/>
    <var def = "reco_shower_dQdx_plane1_median[(reco_shower_ordered_energy_index[(reco_shower_ordered_energy_index[0])])]" binning = "(16,0,400000)" unit = "Median Shower dQ/dx plane 1" type="d" spectator="true"/>
    <var def = "reco_shower_dQdx_plane2_median[(reco_shower_ordered_energy_index[(reco_shower_ordered_energy_index[0])])]" binning = "(16,0,400000)" unit = "Median Shower dQ/dx plane 2" type="d" spectator="true"/>

    <var def = "reco_shower_dQdx_plane0_median[(reco_shower_ordered_energy_index[(reco_shower_ordered_energy_index[1])])]" binning = "(16,0,400000)" unit = "Median Shower dQ/dx plane 0" type="d" spectator="true"/>
    <var def = "reco_shower_dQdx_plane1_median[(reco_shower_ordered_energy_index[(reco_shower_ordered_energy_index[1])])]" binning = "(16,0,400000)" unit = "Median Shower dQ/dx plane 1" type="d" spectator="true"/>
    <var def = "reco_shower_dQdx_plane2_median[(reco_shower_ordered_energy_index[(reco_shower_ordered_energy_index[1])])]" binning = "(16,0,400000)" unit = "Median Shower dQ/dx plane 2" type="d" spectator="true"/>

    <var def = "reco_shower_length[(reco_shower_ordered_energy_index[0])]/(reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]/1000.0)" binning = "(16,0 ,2000 )" unit = "Ratio Reco. Leading Shower Length/Energy [cm/GeV]" type = "d" spectator = "true"/>
    <var def = "reco_shower_length[(reco_shower_ordered_energy_index[1])]/(reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]/1000.0)" binning = "(16,0 ,2000 )" unit = "Ratio Reco. Subleading Shower Length/Energy [cm/GeV]" type = "d" spectator = "true"/>

    <!--Shower Geom Variables-->
    <var def = "(acos(reco_shower_dirx[0]*reco_shower_dirx[1] + reco_shower_diry[0]*reco_shower_diry[1] + reco_shower_dirz[0]*reco_shower_dirz[1]))" binning = "(16,0,3.14)" unit = "Reco. Two-Shower Opening Angle [rad]" type = "d" spectator="false"/>
    <var def = "reco_shower_theta_yz[(reco_shower_ordered_energy_index[0])]" binning = "(16,-3.14,3.14)" unit = "Reco. Leading Shower #theta_{yz} [rad]" type = "d" spectator="true"/>
    <var def = "reco_shower_theta_yz[(reco_shower_ordered_energy_index[1])]" binning = "(16,-3.14,3.14)" unit = "Reco. Subleading Shower #theta_{yz} [rad]" type = "d" spectator="true"/>

    <var def = "reco_shower_length[(reco_shower_ordered_energy_index[0])]" binning = "(16,0,100)" unit = "Leading Shower Length [cm]" type = "d" spectator="true"/>
    <var def = "reco_shower_length[(reco_shower_ordered_energy_index[1])]" binning = "(16,0,100)" unit = "Subleading Shower Length [cm]" type = "d" spectator="true"/>

    <var def = "reco_shower_opening_angle[(reco_shower_ordered_energy_index[0])]" binning = "(16,0,0.8)" unit = "Leading Shower Opening Angle" type = "d" spectator = "true" />
    <var def = "reco_shower_opening_angle[(reco_shower_ordered_energy_index[1])]" binning = "(16,0,0.8)" unit = "Subleading Shower Opening Angle" type = "d" spectator = "true" />

    <var def = "reco_shower_phi_yx[(reco_shower_ordered_energy_index[0])]" binning = "(16,-3.14,3.14)" unit = "Reco. Leading Shower #Phi_{yx}" type = "d" spectator = "true"/>
    <var def = "reco_shower_phi_yx[(reco_shower_ordered_energy_index[1])]" binning = "(16,-3.14,3.14)" unit = "Reco. Subleading Shower #Phi_{yx}" type = "d" spectator = "true"/>

    <var def = "reco_shower_startx[(reco_shower_ordered_energy_index[0])]" binning = "(16,-10,260)" unit = " Reco. Leading Shower start X pos [cm]" type = "d" spectator = "true"/>
    <var def = "reco_shower_starty[(reco_shower_ordered_energy_index[0])]" binning = "(16,-120,+120)" unit = " Reco. Leading Shower start Y pos [cm]" type = "d" spectator = "true"/>
    <var def = "reco_shower_startz[(reco_shower_ordered_energy_index[0])]" binning = "(16,-10,1060)" unit = " Reco. Leading Shower start Z pos [cm]" type = "d" spectator = "true"/>

    <var def = "reco_shower_startx[(reco_shower_ordered_energy_index[1])]" binning = "(16,-10,260)" unit = " Reco. Subleading Shower start X pos [cm]" type = "d" spectator = "true"/>
    <var def = "reco_shower_starty[(reco_shower_ordered_energy_index[1])]" binning = "(16,-120,120)" unit = " Reco. Subleading Shower start Y pos [cm]" type = "d" spectator = "true"/>
    <var def = "reco_shower_startz[(reco_shower_ordered_energy_index[1])]" binning = "(16,-10,1060)" unit = " Reco. Subleading Shower start Z pos [cm]" type = "d" spectator = "true"/>

    <var def = "reco_shower_start_to_nearest_dead_wire_plane0[(reco_shower_ordered_energy_index[0])]" binning = "(16,0,125)" unit = " Reco. Leading Shower dead wire dist plane 0 pos [cm]" type = "d" spectator = "true" />
    <var def = "reco_shower_start_to_nearest_dead_wire_plane1[(reco_shower_ordered_energy_index[0])]" binning = "(16,0,110)" unit = " Reco. Leading Shower dead wire dist plane 1 pos [cm]" type = "d" spectator = "true" />
    <var def = "reco_shower_start_to_nearest_dead_wire_plane2[(reco_shower_ordered_energy_index[0])]" binning = "(16,0,200)" unit = " Reco. Leading Shower dead wire dist plane 2 pos [cm]" type = "d" spectator = "true" />

    <var def = "reco_shower_start_to_nearest_dead_wire_plane0[(reco_shower_ordered_energy_index[1])]" binning = "(16,0,125)" unit = " Reco. Subleading Shower dead wire dist plane 0 pos [cm]" type = "d" spectator = "true" />
    <var def = "reco_shower_start_to_nearest_dead_wire_plane1[(reco_shower_ordered_energy_index[1])]" binning = "(16,0,110)" unit = " Reco. Subleading Shower dead wire dist plane 1 pos [cm]" type = "d" spectator = "true" />
    <var def = "reco_shower_start_to_nearest_dead_wire_plane2[(reco_shower_ordered_energy_index[1])]" binning = "(16,0,200)" unit = " Reco. Subleading Shower dead wire dist plane 2 pos [cm]" type = "d" spectator = "true" />

    <var def =  "reco_shower_nuscore[(reco_shower_ordered_energy_index[0])]" binning = "(16, 0, 1)" unit = "Reco. Leading Shower Associated Slice Neutrino Score" type = "d" spectator = "true" logplot = "true"/>
    <var def =  "reco_shower_nuscore[(reco_shower_ordered_energy_index[1])]" binning = "(16, 0, 1)" unit = "Reco. Subleading Shower Associated Slice Neutrino Score" type = "d" spectator = "true" logplot = "true"/>

    <!-- Pi0 variables -->
    <var def = "(reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]*reco_shower_dirx[(reco_shower_ordered_energy_index[0])]+reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]*reco_shower_dirx[(reco_shower_ordered_energy_index[1])])/1000" binning = "(16, -0.5, 0.5)" unit = "Reco. #pi^{0} x-Momentum [GeV]" type = "d" spectator = "true" />
    <var def = "(reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]*reco_shower_diry[(reco_shower_ordered_energy_index[0])]+reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]*reco_shower_diry[(reco_shower_ordered_energy_index[1])])/1000" binning = "(16, -0.5, 0.5)" unit = "Reco. #pi^{0} y-Momentum [GeV]" type = "d" spectator = "true" />
    <var def = "(reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]*reco_shower_dirz[(reco_shower_ordered_energy_index[0])]+reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]*reco_shower_dirz[(reco_shower_ordered_energy_index[1])])/1000" binning = "(16, -0.5, 0.5)" unit = "Reco. #pi^{0} z-Momentum [GeV]" type = "d" spectator = "true" />

    <!-- Transverse momentum -->
    <var def="sqrt((reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]*reco_shower_diry[(reco_shower_ordered_energy_index[0])] + reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]*reco_shower_diry[(reco_shower_ordered_energy_index[1])])*(reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]*reco_shower_diry[(reco_shower_ordered_energy_index[0])] + reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]*reco_shower_diry[(reco_shower_ordered_energy_index[1])]) + (reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]*reco_shower_dirz[(reco_shower_ordered_energy_index[0])] + reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]*reco_shower_dirz[(reco_shower_ordered_energy_index[1])])*(reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]*reco_shower_dirz[(reco_shower_ordered_energy_index[0])] + reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]*reco_shower_dirz[(reco_shower_ordered_energy_index[1])]))/1000" binning = "(16, 0, 1)" unit = "Reco. #pi^{0} Transverse Momentum [GeV]" type = "d" spectator = "true" />

    <!-- Invariant mass -->
    <var def="sqrt(2.0*reco_shower_energy_plane2[(reco_shower_ordered_energy_index[0])]*reco_shower_energy_plane2[(reco_shower_ordered_energy_index[1])]*(1.0-cos((acos(reco_shower_dirx[0]*reco_shower_dirx[1] + reco_shower_diry[0]*reco_shower_diry[1] + reco_shower_dirz[0]*reco_shower_dirz[1])))))/1000" binning = "(16, 0, 0.25)" unit = "Reconstructed Diphoton Mass [GeV]" type = "d" spectator = "true" />

    <!-- Center-of-mass decay angle -->
